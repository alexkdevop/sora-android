<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AssetView">
        <attr name="assetIcon" format="integer" />
        <attr name="assetBalanceText" format="string" />
        <attr name="assetName" format="string" />
        <attr name="assetIconViewBackgroundColor" format="color" />
        <attr name="assetBlockChainName" format="string" />
    </declare-styleable>

    <declare-styleable name="AssetAmountInputView">
        <attr name="title" format="string" />
        <attr name="tokenSelectorEnabled" format="boolean" />
        <attr name="isBalanceVisible" format="boolean" />
        <attr name="isInputEnabled" format="boolean" />
        <attr name="assetBalance" format="string" />
        <attr name="fiatBalance" format="string" />
    </declare-styleable>

</resources>