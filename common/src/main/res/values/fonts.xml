<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TitleStyle">
        <item name="fontFamily">@font/sora_bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="DialogTitleStyle">
        <item name="android:fontFamily">@font/sora_bold</item>
        <item name="android:textColor">@color/darkGreyBlue</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="GreyTextStyle">
        <item name="fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/lightGrey</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="GreyTextStyle2">
        <item name="fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">13sp</item>
        <item name="android:lineSpacingExtra">7sp</item>
    </style>

    <style name="GreyTextStyle3">
        <item name="fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:textSize">@dimen/greyTextStyleSize</item>
    </style>

    <style name="GreyTextStyle4">
        <item name="fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/grey_500</item>
        <item name="android:textSize">@dimen/text_size_b2</item>
    </style>

    <style name="BlackTextStyle">
        <item name="fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="WhiteTextStyle">
        <item name="android:fontFamily">@font/sora_semibold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="TitleTextWhite">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/sora_semibold</item>
    </style>

    <style name="DarkGreyBlueTitleTextWhite" parent="WhiteTextStyle">
        <item name="android:textColor">@color/titleDarkGreyBlue</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="LightBlackTextStyle">
        <item name="android:fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/lightBlack</item>
        <item name="android:textSize">14sp</item>
    </style>
    
    <style name="BluishGreyTextStyle" parent="LightBlackTextStyle">
        <item name="android:textColor">@color/grey</item>
    </style>

    <style name="VoteButtonTextStyle">
        <item name="android:fontFamily">@font/sora_semibold</item>
        <item name="android:textColor">@color/uikit_lightRed</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="DefaultTextStyle">
        <item name="fontFamily">@font/sora_regular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="PinCodeTitleStyle" parent="DefaultTextStyle">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="DefaultTitleStyle">
        <item name="android:fontFamily">@font/sora_semibold</item>
        <item name="android:textColor">@color/lightBlack</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="ButtonTextWhite">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/sora_semibold</item>
    </style>

    <style name="ButtonTextBlack">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/sora_semibold</item>
    </style>
    
    <style name="LightBlackTitle">
        <item name="android:textColor">@color/lightBlack</item>
    </style>

    <style name="RedTextStyle">
        <item name="android:textColor">@color/uikit_lightRed</item>
        <item name="fontFamily">@font/sora_regular</item>
    </style>

    <style name="TitleTextStyle">
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/sora_bold</item>
    </style>

    <style name="GreenTextStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/green</item>
        <item name="fontFamily">@font/sora_regular</item>
    </style>

    <style name="GreenBlueTextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/titleDarkGreyBlue</item>
        <item name="fontFamily">@font/sora_regular</item>
    </style>

    <style name="BlackListItemTitleStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/sora_regular</item>
    </style>
</resources>